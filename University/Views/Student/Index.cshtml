@{
}
@model List<University.Models.StudentViewModel>
<h1>Student Infomation System</h1>


<div class="row">
    <div class="row-cols-5">
<ul class="list-group">
    @foreach (var student in Model)
    {
        <li class="list-group-item">Id: @student.Id</li>
        <li class="list-group-item">Name: @student.Name</li>
        <li class="list-group-item">Surname: @student.Surname</li>
        <li class="list-group-item">FaxNo.:@student.FaxNo</li>
    }
</ul>
</div>
</div>